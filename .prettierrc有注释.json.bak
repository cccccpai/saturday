{
  "//": "单行长度",
  "printWidth": 120,
  "//": "缩进长度",
  "tabWidth": 2,
  "//": "使用空格代替tab缩进",
  "useTabs": false,
  "//": "句末使用分号",
  "//": "多行时尽可能打印尾随逗号",
  "trailingComma": "none",
  "//": "使用单引号",
  "singleQuote": true,
  "//": "仅在必需时为对象的key添加引号",
  "quoteProps": "as-needed",
  "//": "在对象前后添加空格-eg: { foo: bar }",
  "bracketSpacing": true,
  "//": "在已被preitter格式化的文件顶部加上标注",
  "insertPragma": false,
  "//": "结束行形式",
  "endOfLine": "lf",
  "//": "不对vue中的script及style标签缩进",
  "vueIndentScriptAndStyle": false
}
